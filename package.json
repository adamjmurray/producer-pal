{
  "name": "producer-pal",
  "version": "1.4.3",
  "description": "Producer Pal - AI assistant for music composition inside Ableton Live using an MCP server in a Max for Live device",
  "homepage": "https://producer-pal.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamjmurray/producer-pal.git"
  },
  "type": "module",
  "imports": {
    "#webui/*": "./webui/src/*",
    "#src/*": "./src/*",
    "#evals/*": "./evals/*"
  },
  "author": "Adam Murray",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "music",
    "ai",
    "mcp",
    "ableton"
  ],
  "scripts": {
    "build": "npm run parser:build && npm run ui:build && rollup -c config/rollup.config.mjs && npm run dxt:build",
    "build:all": "ENABLE_RAW_LIVE_API=true ENABLE_CODE_EXEC=true ENABLE_WARP_MARKERS=true npm run build",
    "build:watch": "npm run build && rollup -c config/rollup.config.mjs -w",
    "dev": "npm run parser:build && ENABLE_RAW_LIVE_API=true rollup -c config/rollup.config.mjs -w",
    "dev:all": "ENABLE_CODE_EXEC=true ENABLE_WARP_MARKERS=true npm run dev",
    "test": "npm run test:setup && vitest run --reporter=./config/vitest-minimal-reporter.mjs --silent=true",
    "test:watch": "npm run test:setup && vitest --reporter=dot --silent=true",
    "test:coverage": "npm run test:setup && vitest run --coverage --reporter=./config/vitest-minimal-reporter.mjs --silent=true",
    "test:verbose": "npm run test:setup && vitest run --reporter=default --silent=true",
    "test:coverage:verbose": "npm run test:setup && vitest run --coverage --reporter=default --silent=true",
    "test:setup": "npm run parser:build",
    "coverage": "npm run test:coverage",
    "check": "npm run lint --silent && npm run typecheck --silent && npm run format:check --silent && npm run validate:typescript-only --silent && npm run duplication --silent && npm run test:coverage --silent",
    "check:build": "npm run build && npm run docs:build",
    "fix": "npm run lint:fix && npm run format",
    "parser:build": "peggy --format es --output src/notation/barbeat/parser/generated-barbeat-parser.js src/notation/barbeat/parser/barbeat-grammar.peggy && peggy --format es --output src/notation/transform/parser/generated-transform-parser.js src/notation/transform/parser/transform-grammar.peggy",
    "parser:watch": "chokidar 'src/**/*.peggy' -c 'npm run parser:build'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "echo 'Linting...' && eslint --concurrency=auto . && echo 'No lint violations.\n'",
    "lint:fix": "npm run lint -- --fix",
    "duplication": "npm run duplication:src --silent && npm run duplication:tests --silent && npm run duplication:scripts --silent && npm run duplication:evals --silent && npm run duplication:e2e --silent",
    "duplication:src": "printf 'Source ' && jscpd -s -c config/.jscpd.json",
    "duplication:tests": "printf 'Tests ' && jscpd -s -c config/.jscpd-tests.json",
    "duplication:scripts": "printf 'Scripts ' && jscpd -s -c config/.jscpd-scripts.json",
    "duplication:evals": "printf 'Evals ' && jscpd -s -c config/.jscpd-evals.json",
    "duplication:e2e": "printf 'E2E ' && jscpd -s -c config/.jscpd-e2e.json",
    "duplication:verbose": "jscpd -c config/.jscpd.json && jscpd -c config/.jscpd-tests.json && jscpd -c config/.jscpd-scripts.json && jscpd -c config/.jscpd-evals.json && jscpd -c config/.jscpd-e2e.json",
    "loc": "cloc --vcs=git --not-match-f='package-lock.json|-parser\\.js$'",
    "knowledge-base": "node scripts/build-and-release/generate-knowledge-base.ts",
    "kb": "npm run knowledge-base",
    "dxt:clean": "rm -f claude-desktop-extension/*.mcpb claude-desktop-extension/*.js claude-desktop-extension/manifest.json claude-desktop-extension/LICENSE",
    "dxt:build": "node scripts/build-and-release/build-claude-desktop-extension.ts",
    "version:bump": "node scripts/build-and-release/bump-version.ts",
    "version:bump:minor": "node scripts/build-and-release/bump-version.ts minor",
    "version:bump:major": "node scripts/build-and-release/bump-version.ts major",
    "release": "node scripts/build-and-release/prepare-release.ts",
    "tc": "npm run typecheck",
    "typecheck": "echo 'Typechecking...' && npm run typecheck:src --silent && npm run typecheck:scripts --silent && npm run typecheck:evals --silent && npm run typecheck:webui --silent && npm run typecheck:e2e --silent && echo 'No typecheck violations.\n'",
    "typecheck:src": "tsc --project src/tsconfig.json",
    "typecheck:scripts": "tsc --project scripts/tsconfig.json",
    "typecheck:evals": "tsc --project evals/tsconfig.json",
    "typecheck:webui": "tsc --project webui/tsconfig.json",
    "typecheck:e2e": "tsc --project e2e/webui/tsconfig.json && tsc --project e2e/mcp/tsconfig.json && tsc --project e2e/docs/tsconfig.json",
    "ui:build": "vite build --config config/vite.config.ts",
    "ui:dev": "ENABLE_RAW_LIVE_API=true ENABLE_CODE_EXEC=true ENABLE_WARP_MARKERS=true vite --config config/vite.config.ts",
    "ui:test": "node --env-file=.env node_modules/.bin/playwright test -c config/playwright.ui.config.ts",
    "ui:test:dev": "node --env-file=.env node_modules/.bin/playwright test -c config/playwright.ui.config.ts --ui",
    "ui:test:headed": "node --env-file=.env node_modules/.bin/playwright test -c config/playwright.ui.config.ts --headed",
    "validate:typescript-only": "node scripts/build-and-release/validate-typescript-only.ts",
    "docs:dev": "vitepress dev docs --port 5174",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:test": "npm run docs:build && playwright test -c config/playwright.docs.config.ts",
    "docs:test:dev": "npm run docs:build && playwright test -c config/playwright.docs.config.ts --ui",
    "docs:test:headed": "npm run docs:build && playwright test -c config/playwright.docs.config.ts --headed",
    "e2e:docs": "npm run docs:test",
    "e2e:mcp": "vitest run -c config/vitest.e2e.config.ts",
    "e2e:mcp:watch": "vitest -c config/vitest.e2e.config.ts",
    "e2e:webui": "npm run ui:test"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.37",
    "@anthropic-ai/sdk": "^0.78.0",
    "@google/genai": "^1.43.0",
    "@modelcontextprotocol/sdk": "^1.27.1",
    "@tailwindcss/typography": "^0.5.19",
    "ai": "^6.0.105",
    "express": "^5.2.1",
    "marked": "^17.0.3",
    "openai": "^6.25.0",
    "preact": "^10.28.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@anthropic-ai/mcpb": "^2.1.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.7.0",
    "@playwright/test": "^1.58.2",
    "@preact/preset-vite": "^2.10.3",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@tailwindcss/vite": "^4.2.1",
    "@testing-library/preact": "^3.2.4",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "chokidar-cli": "^3.0.0",
    "commander": "^14.0.3",
    "eslint": "^9.39.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.7.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sonarjs": "^4.0.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "happy-dom": "^20.7.0",
    "jscpd": "^4.0.8",
    "peggy": "^5.1.0",
    "prettier": "^3.8.1",
    "rollup": "^4.59.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-singlefile": "^2.3.0",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.17"
  }
}
